        android:supportsRtl="true"
        android:theme="@style/Theme.Ships_version2"
        tools:targetApi="31">

        <activity
            android:name=".AppCompactAtcivity.AuthorizationActivity"
            android:exported="false"
            android:label="@string/title_activity_register"
