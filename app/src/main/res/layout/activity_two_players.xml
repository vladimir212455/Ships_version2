        android:layout_height="40pt"
        android:src="@color/black"
        app:layout_constraintLeft_toRightOf="@+id/button_3_2"
        app:layout_constraintTop_toBottomOf="@+id/button_2_3"
        tools:ignore="MissingConstraints"
        android:scaleType="centerCrop"
        />

 <TextView
     android:id="@+id/hp_first"
     android:layout_width="wrap_content"
     android:layout_height="wrap_content"
     android:text="hp_player1"
     app:layout_constraintStart_toStartOf="parent"
     app:layout_constraintTop_toBottomOf="@+id/button_3_0"
     tools:ignore="MissingConstraints" />

 <TextView
     android:id="@+id/hp_second"
     android:layout_width="wrap_content"
     android:layout_height="wrap_content"
     android:text="hp_player2"
     app:layout_constraintEnd_toEndOf="parent"
     app:layout_constraintTop_toBottomOf="@+id/button_3_3"
     tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>