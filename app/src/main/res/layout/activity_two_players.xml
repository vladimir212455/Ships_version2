        android:scaleType="centerCrop"
        />

    <ImageButton
        android:id="@+id/button_1_1"
        android:layout_width="40pt"
        android:layout_height="40pt"
        android:src="@color/black"
        app:layout_constraintLeft_toRightOf="@+id/button_1_0"
